                "position": "fixed",
                "top": "50%",
                "font-weight": "normal",
                "-webkit-transform": "translateY(-50%)",
                "-ms-transform": "translateY(-50%)",
                "transform": "translateY(-50%)"
              },
              "& when (@showFloat = false)": {
                ".icon-bar a:hover": {
                  "background-color": "@FloatColor"
                }
              },
              "& when (@showFloat)": {
                ".icon-bar a:hover": {
                  "background-color": "@FloatColor",
                  "margin-left": "-50px",
                  "width": "100px"
                }
              }
            },
            "& when (@showPossSocial)": {
              ".icon-bar": {
                "z-index": "100002",
                "left": "(@SocialSpacing * 1%)",
                "position": "fixed",
                "top": "50%",
                "font-weight": "normal",
                "-webkit-transform": "translateY(-50%)",
                "-ms-transform": "translateY(-50%)",
                "transform": "translateY(-50%)"
              },
              "& when (@showFloat)": {
                ".icon-bar a:hover": {
                  "background-color": "@FloatColor",
                  "width": "100px"
                }
              },
              "& when (@showFloat = false)": {
                ".icon-bar a:hover": {
                  "background-color": "@FloatColor"
                }
              }
            },
            ".icon-bar a": {
              "display": "block",
              "text-align": "center",
              "padding": "10px",
              "transition": "all 0.3s ease",
              "color": "white",
              "font-size": "20px",
              "font-weight": "normal",
              "margin": "(@SocialSpace * 1px)",
              "border-radius": "(@SocialRadius * 1px)"
            },
            ".facebook": {
              "background": "#3B5998",
              "color": "white",
              "width": "50px"
            },
            ".twitter": {
              "background": "#55ACEE",
              "color": "white",
              "width": "50px"
            },
            ".google": {
              "background": "#dd4b39",
              "color": "white",
              "width": "50px"
            },
            ".linkedin": {
              "background": "#007bb5",
              "color": "white",
              "width": "50px"
            },
            ".youtube": {
              "background": "#bb0000",
              "color": "white",
              "width": "50px"
