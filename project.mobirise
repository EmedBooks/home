          "_name": "design-block",
          "_customHTML": "<section mbr-id=\"_anchor\" class=\"_customHTML\">\n \n  \n    <mbr-parameters>\n<hr><header>Social Config</header>\n      <input type=\"checkbox\" title=\"Show Social Button\" name=\"showSocial\" checked>\n      <input type=\"checkbox\" title=\"Right/Left\" name=\"showPossSocial\" condition=\"showSocial\" checked>\n      <input type=\"checkbox\" title=\"Float Over\" name=\"showFloat\" condition=\"showSocial\" checked>\n       <input type=\"color\" title=\"Float Color\" name=\"FloatColor\" value=\"#ff0040\" condition=\"showSocial\">\n       <input type=\"range\" title=\"Radius\" name=\"SocialRadius\" min=\"0\" max=\"25\" step=\"1\" value=\"22\" condition=\"showSocial\">\n       <input type=\"range\" title=\"Spacing\" name=\"SocialSpace\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"showSocial\">\n       <input type=\"range\" title=\"Tab\" name=\"SocialSpacing\" min=\"0\" max=\"3\" step=\"0.2\" value=\"0.6\" condition=\"showSocial\">\n      <input type=\"checkbox\" title=\"Facebook\" name=\"showFB\" condition=\"showSocial\" checked>\n      <input type=\"text\" name=\"Facebook\" title=\"Link\" value=\"https://www.facebook.com/Digitalmedicalbooks/\" condition=\"showFB&&showSocial\">\n      <input type=\"checkbox\" title=\"Twitter\" name=\"showTW\" condition=\"showSocial\">\n      <input type=\"text\" name=\"Twitter\" title=\"Link\" value=\"https://www.twitter.com/\" condition=\"showTW&&showSocial\">\n      <input type=\"checkbox\" title=\"Google\" name=\"showGG\" condition=\"showSocial\">\n      <input type=\"text\" name=\"Google\" title=\"Link\" value=\"https://www.google.com/\" condition=\"showGG&&showSocial\">\n      <input type=\"checkbox\" title=\"Linkedin\" name=\"showLK\" condition=\"showSocial\">\n      <input type=\"text\" name=\"Linkedin\" title=\"Link\" value=\"https://www.linkedin.com/\" condition=\"showLK&&showSocial\">\n      <input type=\"checkbox\" title=\"Youtube\" name=\"showYT\" condition=\"showSocial\">\n      <input type=\"text\" name=\"Youtube\" title=\"Link\" value=\"https://www.youtube.com/\" condition=\"showYT&&showSocial\">\n      <input type=\"checkbox\" title=\"Instagram\" name=\"showIT\" condition=\"showSocial\">\n      <input type=\"text\" name=\"Instagram\" title=\"Link\" value=\"https://www.instagram.com/\" condition=\"showIT&&showSocial\">\n      <input type=\"checkbox\" title=\"Custom 1\" name=\"showC1\" condition=\"showSocial\">\n                    <input type=\"color\" title=\"Background Color\" name=\"CustomColor1\" value=\"#ff7300\" condition=\"showC1&&showSocial\">\n      <input type=\"checkbox\" title=\"Custom 2\" name=\"showC2\" condition=\"showSocial\">\n                    <input type=\"color\" title=\"Background Color\" name=\"CustomColor2\" value=\"#01bf46\" condition=\"showC2&&showSocial\">\n\n      <header>Ribbon Config</header>\n      <input type=\"checkbox\" title=\"Show Ribbon\" name=\"Ribbon\">\n      <select title=\"Position\" name=\"RibbonPoss\" condition=\"Ribbon\">\n            <option value=\"LeftTop\" selected>Top Left</option>\n            <option value=\"RightTop\">Top Right</option>\n            <option value=\"LeftBottom\">Bottom Left</option>\n            <option value=\"RightBottom\">Bottom Right</option>\n      </select>\n       <input type=\"color\" title=\"Background Color 1\" name=\"RibbonColor\" value=\"#55b4d4\" condition=\"Ribbon\">\n        <input type=\"color\" title=\"Background Color 2\" name=\"RibbonColor1\" value=\"#000000\" condition=\"Ribbon\">\n\n      \n     <header>Float Menu</header>\n\n       <input type=\"checkbox\" title=\"Show Social Bar\" name=\"Socbar\">     \n        <input type=\"range\" inline title=\"Top\" name=\"paddingTopmenu\" min=\"0\" max=\"400\" step=\"1\" value=\"400\" condition=\"Socbar\">\n        <input type=\"range\" inline title=\"Width\" name=\"widthmenu\" min=\"200\" max=\"600\" step=\"1\" value=\"200\" condition=\"Socbar\">\n        <input type=\"range\" inline title=\"Width Hover\" name=\"widthover\" min=\"100\" max=\"1000\" step=\"1\" value=\"100\" condition=\"Socbar\">\n        <input type=\"range\" inline title=\"Thickness\" name=\"heightmenu\" min=\"43\" max=\"100\" step=\"1\" value=\"43\" condition=\"Socbar\">\n        <input type=\"color\" title=\"Background Color\" name=\"bgcolormenu\" value=\"#cccccc\" condition=\"Socbar\">\n        <input type=\"range\" inline title=\"Items Count\" name=\"iconsCount\" min=\"1\" max=\"9\" step=\"1\" value=\"5\" condition=\"Socbar\">\n        <input type=\"checkbox\" title=\"Grayscale\" name=\"Grayscale\" condition=\"Socbar\">\n      \n    </mbr-parameters> \n  <div class=\"sticky-container\" mbr-if=\"Socbar\">\n\t\t<ul class=\"sticky\">\n\n                        \t\t\t<li>\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 1</a></p>\n\t\t\t</li>\n            <li mbr-if=\"iconsCount > 1\">\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 2</a></p>\n\t\t\t</li>\n            \t\t\t<li mbr-if=\"iconsCount > 2\">\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 3</a></p>\n\t\t\t</li>\n            \t\t\t<li mbr-if=\"iconsCount > 3\">\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 4</a></p>\n\t\t\t</li>\n            \t\t\t<li mbr-if=\"iconsCount > 4\">\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 5</a></p>\n\t\t\t</li>\n\t\t\t<li mbr-if=\"iconsCount > 5\">\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 6</a></p>\n\t\t\t</li>\n\t\t\t<li mbr-if=\"iconsCount > 6\">\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 7</a></p>\n                </li>\n\t\t\t<li mbr-if=\"iconsCount > 7\">\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 8</a></p>\n</li>\n            <li mbr-if=\"iconsCount > 8\">\n     \n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 9</a></p>\n\t\t\t</li>\n          \t\t\t<li mbr-if=\"iconsCount > 9\">\n\t\t\t\t<img width=\"32\" height=\"32\" title alt src=\"http://unsitepourtous.be/mobi/mbr-64x78.png\">\n\t\t\t\t<p><a href=\"#top\" class=\"text-primary\" data-app-placeholder=\"Type Text\">Page 10</a></p>\n\t\t\t</li>\n\n\t\t</ul>\n\t</div>\n  \n<div mbr-if=\"Ribbon\">\n  <div class=\"corner-ribbon effetover top-zen\" mbr-text><ov>FACEBOOK</ov></div>\n</div>              \n      \n\n<div class=\"icon-bar\" mbr-if=\"showSocial\">\n  <a href=\"{{Facebook}}\" class=\"facebook\" mbr-if=\"showFB\"><z class=\"mbr-iconfont socicon-facebook socicon\" mbr-if=\"showFB\"></z></a> \n  <a href=\"{{Twitter}}\" class=\"twitter\" mbr-if=\"showTW\"><z class=\"mbr-iconfont socicon-twitter socicon\" mbr-if=\"showTW\"></z></a> \n  <a href=\"{{Google}}\" class=\"google\" mbr-if=\"showGG\"><z class=\"mbr-iconfont socicon-google socicon\" mbr-if=\"showGG\"></z></a> \n  <a href=\"{{Linkedin}}\" class=\"linkedin\" mbr-if=\"showLK\"><z class=\"mbr-iconfont socicon-linkedin socicon\" mbr-if=\"showLK\"></z></a>\n  <a href=\"{{Youtube}}\" class=\"youtube\" mbr-if=\"showYT\"><z class=\"mbr-iconfont socicon-youtube socicon\" mbr-if=\"showYT\"></z></a> \n  <a href=\"{{Instagram}}\" class=\"instagram\" mbr-if=\"showIT\"><z class=\"fa fa-instagram\" mbr-if=\"showIT\"></z></a> \n  <a href=\"https://www.google.com\" class=\"custom1\" mbr-if=\"showC1\"><z mbr-if=\"showC1\"><span mbr-icon class=\"mbr-iconfont mbri-plus\" style=\"font-size: 20px; color: rgb(35, 35, 35); fill: rgb(35, 35, 35);\"></span></z></a>\n  <a href=\"https://www.google.com\" class=\"custom2\" mbr-if=\"showC2\"><z mbr-if=\"showC2\"><span mbr-icon class=\"mbr-iconfont mbri-plus\" style=\"font-size: 20px;\"></span></z></a>\n </div>  \n\n <div mbr-class=\"{'hidden': isPublish}\" style=\"padding:1rem; background-color:#c1c1c1;\">\n   <div class=\"align-center\">\n              <h10> <br><br><br><br><br><br>Floating Social Menu and Ribbons<br><br>\n        Click the \"gear\" icon to change the layout of the social bar. This text will be removed on preview/publish.<br><br><svg id=\"svg\" version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" width=\"150\" viewBox=\"0, 0, 400,383.33333333333337\"><g id=\"svgg\"><path id=\"path0\" d=\"M37.210 6.152 C 35.603 6.521,34.673 7.586,34.092 9.720 C 33.330 12.516,33.825 95.736,34.612 97.241 C 35.870 99.644,28.382 99.467,134.420 99.600 C 199.138 99.681,231.925 99.852,232.882 100.115 C 235.440 100.816,235.350 101.661,230.295 124.449 C 224.368 151.170,212.070 208.952,209.422 222.521 C 207.800 230.833,207.905 232.790,209.971 232.790 C 211.891 232.790,209.325 235.910,254.122 179.099 C 277.812 149.054,289.953 133.596,295.689 126.174 C 297.890 123.327,306.521 112.368,314.870 101.822 C 367.957 34.759,386.363 11.054,386.765 9.224 C 387.073 7.820,386.446 6.596,385.169 6.111 C 383.972 5.656,39.193 5.697,37.210 6.152 M382.9