"checkbox\" title=\"Image 2\" name=\"mediaup\">\n              <input type=\"range\" inline title=\"Size\" name=\"mediaSizeup\" min=\"10\" max=\"100\" step=\"5\" value=\"10\" condition=\"mediaup\">\n         <input type=\"checkbox\" title=\"Image 3\" name=\"mediadw\">\n        <input type=\"range\" inline title=\"Size\" name=\"mediaSizedw\" min=\"10\" max=\"100\" step=\"5\" value=\"10\" condition=\"mediadw\">\n        <input type=\"range\" inline title=\"Radius\" name=\"radius\" min=\"0\" max=\"1000\" step=\"1\" value=\"63\" condition=\"mediadw || mediaup || mediatop\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/mbr-1920x1080.jpg\" parallax selected>\n            <input type=\"color\" title=\"Background Color\" value=\"#232323\">\n            <input type=\"video\" title=\"Background Video\" value=\"http://www.youtube.com/watch?v=uNCr7NdOJgw\">\n        </fieldset>\n        \n         <input type=\"color\" title=\"Color 2\" name=\"color2\" value=\"#ff3366\" condition=\"bg.type == 'color'\">\n         <input type=\"range\" name=\"angle\" title=\"Angle\" min=\"0\" max=\"270\" value=\"0\" step=\"1\" condition=\"bg.type == 'color'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232323\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"overlay && bg.type !== 'color'\">\n\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\">\n    </div>\n    \n    <div class=\"container text-center\" data-stellar-background-r